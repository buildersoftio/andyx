{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss}  andyx  {Level,-12} {0,-3} | {0,-3} {Message}{NewLine}{Exception}"
        }
      }
    ]
  },

  "NodeId": "default_01",

  "Credentials": {
    "Username": "admin",
    "Password": "admin"
  },

  "Threads": {
    "MaxNumber": 4,
    "MinNumber": 1,
    "LoadBalanced": false
  },
  "AllowedHosts": "*"
}
